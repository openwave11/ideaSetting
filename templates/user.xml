<templateSet group="user">
  <template name="jdk1.8" value="&lt;plugin&gt;&#10;    &lt;groupId&gt;org.apache.maven.plugins&lt;/groupId&gt;&#10;    &lt;artifactId&gt;maven-compiler-plugin&lt;/artifactId&gt;&#10;    &lt;version&gt;3.8.0&lt;/version&gt;&#10;    &lt;configuration&gt;&#10;        &lt;target&gt;1.8&lt;/target&gt;&#10;        &lt;source&gt;1.8&lt;/source&gt;&#10;        &lt;encoding&gt;UTF-8&lt;/encoding&gt;&#10;    &lt;/configuration&gt;&#10;&lt;/plugin&gt;" description="编译为jdk1.8" toReformat="false" toShortenFQNames="true">
