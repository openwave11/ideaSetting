  <template name="tomcat7" value="  &lt;plugin&gt;&#10;    &lt;groupId&gt;org.apache.tomcat.maven&lt;/groupId&gt;&#10;    &lt;artifactId&gt;tomcat7-maven-plugin&lt;/artifactId&gt;&#10;    &lt;version&gt;2.2&lt;/version&gt;&#10;    &lt;configuration&gt;&#10;      &lt;port&gt;8888&lt;/port&gt;&#10;    &lt;/configuration&gt;&#10;  &lt;/plugin&gt;" description="设置tomcat7" toReformat="false" toShortenFQNames="true">
