      <option name="XML" value="true" />
    </context>
  </template>
  <template name="ng-model" value="ng-model=&quot;$END$&quot;" description="ng-module数据模型" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="true" />
      <option name="XML" value="true" />
    </context>
  </template>
  <template name="app" value="var app = angular.module('$appName$', []);" description="var app = angular.module('$appName$', []);" toReformat="false" toShortenFQNames="true">
    <variable name="appName" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="true" />
      <option name="XML" value="true" />
    </context>
  </template>
  <template name="$scope." value="$scope.$END$&quot;" description="$scope." toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="true" />
      <option name="XML" value="true" />
    </context>
  </template>
  <template name="/*" value="/**&#10; * @Description $end$ &#10; * @Date $time$ $date$&#10; * @Author $author$&#10; * @Param $param$&#10; * @return $return$&#10; */" description="" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="author" expression="user()" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="methodParameters()" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ng-click" value="ng-click=&quot;$END$&quot;" description="ng-click" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="true" />
      <option name="XML" value="true" />
    </context>
  </template>
</templateSet>